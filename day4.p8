pico-8 cartridge // http://www.pico-8.com
version 38
__lua__

function _init()
	cls()
	w=128
	h=128
	-- printh("starting up", "pico8.txt", true, true)
end

function _draw()
	for x=0,127 do
		for y=0,127 do
			tm=t()
			c = sin((x + tm)/80) + sin((y+tm)/80)
			
			if c < 0.01 then
				c *= 100
			elseif c < 1 then
				c *= 10
			end

			c1 = c*4
			pset(x,y, c1)
		end
	end
end


